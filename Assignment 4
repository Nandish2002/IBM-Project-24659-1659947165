 (3, "Tomatoes", 2),
 (4, "Taco Shell", 2),
 (5, "Cheese", 3),
 (6, "Milk", 1),
 (7, "Bread", 2);
4.CREATE TABLE recipe_ingredients (
 recipe_id int NOT NULL, 
